enable_mysql_tests='@enable_mysql_tests@'
host='@host@'
TIMEOUT='@TIMEOUT@'
mysqlvars=`echo $enable_mysql_tests | awk '
  BEGIN { FS=":" }
  /^(yes|no)$/ { next }
  length($1) { printf "mysqluser='\''%s'\'' ", $1 }
  length($2) { printf "mysqlpassword='\''%s'\'' ", $2 }
  length($3) { printf "mysqldb='\''%s'\'' ", $3 }
' `

